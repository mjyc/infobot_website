{
  // Adopted from: https://github.com/RobotWebTools/starter-template/blob/stable/utils/.jshintrc
  "globals": {
    // jquery
    "$": true,
    "jQuery": true,
    // misc
    "__dirname": true,
    "process": true,
    "require": true,
    "Switchery": true,
    "queryjobCards":true,
    "home":true,
    "urlROS":true,
    // test
    "after": true,
